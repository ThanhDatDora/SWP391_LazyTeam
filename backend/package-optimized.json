{
  "name": "mini-coursera-backend-optimized",
  "version": "1.0.0",
  "description": "Optimized backend server for Mini Coursera platform with caching, rate limiting, and performance monitoring",
  "main": "server-optimized.js",
  "scripts": {
    "start": "node server-optimized.js",
    "dev": "nodemon server-optimized.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "db:migrate": "node scripts/migrate.js up",
    "db:rollback": "node scripts/migrate.js down",
    "db:optimize": "sqlcmd -S localhost -d MiniCoursera -i database/optimization.sql",
    "cache:clear": "node scripts/clear-cache.js",
    "performance:test": "k6 run tests/performance/load-test.js",
    "docker:build": "docker build -t mini-coursera-backend .",
    "docker:run": "docker run -p 3000:3000 mini-coursera-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^1.6.0",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.6",
    "http-proxy-middleware": "^2.0.6",
    "redis": "^4.6.10",
    "node-cache": "^5.1.2",
    "mssql": "^10.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.14.0",
    "socket.io": "^4.7.4",
    "stripe": "^14.7.0",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "coursera",
    "education",
    "api",
    "optimized",
    "caching",
    "performance"
  ],
  "author": "Mini Coursera Team",
  "license": "MIT"
}