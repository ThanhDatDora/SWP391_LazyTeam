{
  "name": "mini-coursera-ui-tailwind",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "vite build",
    "build:analyze": "vite build --config vite.config.performance.js",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "preview:prod": "vite build --mode production && vite preview",
    "check-quality": "node scripts/check-quality.js",
    "fix:navigation": "node scripts/fix-navigation.js",
    "analyze": "npm run check-quality && npm run build",
    "performance:analyze": "npm run build:analyze",
    "performance:test": "node scripts/performance-test.js",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:all": "npm run test:jest && npm run test:run",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.0.0",
    "class-variance-authority": "^0.7.0",
    "lucide-react": "^0.441.0",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.23.0",
    "recharts": "^3.2.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@eslint/js": "^9.37.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^30.2.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.37.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.4",
    "postcss": "^8.4.41",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.10",
    "terser": "^5.31.0",
    "vite": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "vitest related --run --reporter=verbose"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
